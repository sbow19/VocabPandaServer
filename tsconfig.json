{
    "compilerOptions": {
      "target": "ESNext",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "declaration": true,
      "types": [
        "node",
        "express",
        "body-parser"],
      "baseUrl": "./",
      "paths": {
        "@appTypes/*" : ["src/appTypes/*"],
        "@app/*": ["src/app/*"],
  "@shared/*": ["src/shared/*"],
  "@vpModels/*": ["src/app/models/*"],
  "@website/*": ["src/website/*"]
      }
    }
  }